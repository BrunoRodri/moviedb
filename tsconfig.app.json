{
  "compilerOptions": {
    "target": "ESNext", 
    "module": "ESNext", 
    "moduleResolution": "node",
    "types": ["vite/client", "swiper"], 
    "jsx": "react-jsx", 
    "strict": true, 
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true, 
    "outDir": "./dist", 
    "rootDir": "./src" 
  },
  "include": ["src/**/*"], 
  "exclude": ["node_modules"] 
}